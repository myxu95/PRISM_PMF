; em.mdp - Energy minimization
title           = Energy minimization
define          = -DPOSRES -DPOSRES_FC_BB=400.0 -DPOSRES_FC_SC=40.0
integrator      = steep
emtol           = 200.0
emstep          = 0.01
nsteps          = 10000

nstxout         = 0
nstvout         = 0
nstenergy       = 0
nstlog          = 0
nstxout-compressed = 0

cutoff-scheme   = Verlet
coulombtype     = PME
rcoulomb        = 1.0
rvdw            = 1.0
pbc             = xyz
